#include <windows.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION     @SOKKETTER_VERSION_MAJOR@,@SOKKETTER_VERSION_MINOR@,@SOKKETTER_VERSION_MICRO@,@SOKKETTER_VERSION_NANO@
 PRODUCTVERSION  @SOKKETTER_VERSION_MAJOR@,@SOKKETTER_VERSION_MINOR@,@SOKKETTER_VERSION_MICRO@,@SOKKETTER_VERSION_NANO@
 FILEFLAGSMASK   @SOKKETTER_BINARY_TYPE@
 FILEFLAGS       0x0L
 FILEOS          0x40004L
 FILETYPE        0x1L
 FILESUBTYPE     0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "@SOKKETTER_COMPANY@"
            VALUE "FileDescription", "@SOKKETTER_DESCRIPTION@"
            VALUE "FileVersion", "@SOKKETTER_VERSION_MAJOR@.@SOKKETTER_VERSION_MINOR@.@SOKKETTER_VERSION_MICRO@.@SOKKETTER_VERSION_NANO@"
            VALUE "InternalName", "@SOKKETTER_NAME@"
            VALUE "LegalCopyright", "@SOKKETTER_COPYRIGHT@"
            VALUE "OriginalFilename", "@SOKKETTER_EXECUTABLE@"
            VALUE "ProductName", "@SOKKETTER_NAME@"
            VALUE "ProductVersion", "@SOKKETTER_VERSION_MAJOR@.@SOKKETTER_VERSION_MINOR@.@SOKKETTER_VERSION_MICRO@.@SOKKETTER_VERSION_NANO@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END
